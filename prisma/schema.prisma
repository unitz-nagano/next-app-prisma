generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Account {
  user_id   Int    @id @default(autoincrement())
  type      String
  full_name String
  e_mail    String
  user_type Int
}

model Test {
  id    Int    @id @default(autoincrement())
  value String
}
